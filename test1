Object.entries(sidebarData).map(([sectionName, sectionDataRaw]) => {
  const sectionData = sectionDataRaw as {
    id: string;
    SubCategoryList: Record<
      string,
      {
        id: string;
        count: number;
      }
    >;
  };

  const subItems =
    sectionData?.SubCategoryList &&
    Object.entries(sectionData.SubCategoryList).map(([subLabel, subData]) => ({
      label: subLabel,
      count: subData.count,
      href: "/", // update as needed
    }));

  return (
    <NavItem
      key={sectionData.id}
      icon="./menuIcon.svg"
      label={sectionName}
      collapsed={collapsed}
      subItems={subItems}
    />
  );
});
