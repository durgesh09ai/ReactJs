  fetchSolutionCatalogueData = async (getId?:string,getSolType?:string,offset?:number,limit?:number) => {
    try {
      this.catalogloading = true;
      const setOffset = offset ?? 0;
      const setLimit = limit ?? 12;
      if(getId=='all' && getSolType=='all' ){ 
      const res = await fetch(`${this.apiUrl}/soln_catalogue/page_solns?offset=${setOffset}&limit=${setLimit}`);
     }else{
     const res = await fetch(`${this.apiUrl}/soln_catalogue/soln_leftmenu?offset=${setOffset}&limit=${setLimit}`);
      }
    
      if (!res.ok) throw new Error("Failed to fetch datastate data");
      const data = await res.json();
      runInAction(() => {
        this.catalogloading = false;
        this.solutionCatalogueData = data;
      });
    } catch (e: any) {
      runInAction(() => {
        this.error = e.message;
      });
    }
  };

}
